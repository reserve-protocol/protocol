{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "useTabs": false,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "singleQuote": false,
        "tabWidth": 4
      }
    },
    {
      "files": "*.json",
      "options": {
        "singleQuote": false,
        "tabWidth": 2
      }
    }
  ]
}
